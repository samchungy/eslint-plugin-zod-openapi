{
  "compilerOptions": {
    "lib": ["ES2022"],
    "outDir": "lib",
    "target": "ES2022",
    "removeComments": false,
    "module": "node16",
    "skipLibCheck": true,
    "incremental": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "exclude": ["lib*/**/*"],
  "extends": "tsconfig-seek"
}
